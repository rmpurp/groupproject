{
  "name": "groupproject",
  "version": "1.0.0",
  "description": "Extension for managing your Chrome extensions.",
  "scripts": {
    "build": "tsc && cp public/* dist/",
    "dev": "nodemon --exec \"npm run build || exit 1\" --ignore dist/ --ext html,json,png,ts,css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmpurp/groupproject.git"
  },
  "author": "Ryan Purpura",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/rmpurp/groupproject/issues"
  },
  "homepage": "https://github.com/rmpurp/groupproject#readme",
  "devDependencies": {
    "@types/chrome": "^0.0.316",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3"
  }
}